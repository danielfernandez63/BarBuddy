@{
    ViewBag.Title = "Home Page";
}
<body style="background-image: url('/Images/Bbackground.png');">
    @*background-repeat:no-repeat background-attachment:fixed>*@


    <br />


    <div>
        <div id="myCarousel" class="carousel" data-ride="carousel" style="overflow: hidden">
            <ol class="carousel-indicators">
                <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                <li data-target="#myCarousel" data-slide-to="1"></li>
                <li data-target="#myCarousel" data-slide-to="1"></li>
            </ol>

            <div class="carousel-inner">
                <div class="item active">
                    <img src="~/Images/Bar1.jpg" alt="Bar1" style="width:100%; height: 600px"/>
                </div>
                <div class="item">
                    <img src="~/Images/bar2.png" alt="Bar2" style="width:100%; height: 600px"/>
                </div>
                <div class="item">
                    <img src="~/Images/bar3.jpg" alt="Bar3" style="width:100%; height: 600px"/>
                </div>
                <div class="item">
                    <img src="~/Images/bar4.jpg" alt="Bar4" style="width:100%; height: 600px"/>
                </div>
            </div>


            <a class="left carousel-control" data-target="#myCarousel" data-slide="prev">
                <span class="glyphicon glyphicon-chevron-left"> </span>"
                <span class="sr-only">Previous</span>
            </a>
            <a class="right carousel-control" data-target="#myCarousel" data-slide="next">
                <span class="glyphicon glyphicon-chevron-right"> </span>"
                <span class="sr-only">Next</span>
            </a>      
        </div>
    </div>
    <br />
    <p class="lead">It never hurts to have a buddy with you, why not take yours to work! </p>
